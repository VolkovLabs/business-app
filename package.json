{
  "author": "Volkov Labs",
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@grafana/data": "^11.2.0",
    "@grafana/runtime": "^11.2.0",
    "@grafana/ui": "^11.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^5.3.4",
    "tslib": "^2.7.0"
  },
  "description": "Business App for Grafana",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/plugin-e2e": "^1.9.0",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.46.1",
    "@swc/core": "^1.7.18",
    "@swc/helpers": "^0.5.12",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/node": "^22.5.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@volkovlabs/eslint-config": "^1.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "license": "Apache-2.0",
  "name": "business-suite",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "levitate": "npx @grafana/levitate@latest is-compatible --path src/module.ts --target @grafana/data,@grafana/runtime,@grafana/ui",
    "lint": "eslint --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "sign": "npx --yes @grafana/sign-plugin@latest --rootUrls http://localhost:3000/",
    "start": "docker compose build && docker compose up",
    "start:e2e": "docker compose -f test/docker-compose.yml up",
    "stop": "docker compose down",
    "stop:e2e": "docker compose -f test/docker-compose.yml down",
    "test": "jest --watch --onlyChanged",
    "test:e2e": "npx playwright test",
    "test:ci": "jest --maxWorkers 4 --coverage",
    "upgrade": "npm upgrade --save"
  },
  "version": "4.4.0"
}
